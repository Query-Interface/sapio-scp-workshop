<mvc:View
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc"
   controllerName="sap.io.demo.project.controller.ProjectList">
<Page title="{i18n>projectPageTitle}">
    <content>
        <List
            class="sapUiResponsiveMargin"
            width="auto"
            items="{/projects}" >
            <items>
                <ObjectListItem
                    title="{id} | {name}"/>
            </items>
        </List>
    </content>
</Page>
</mvc:View>
